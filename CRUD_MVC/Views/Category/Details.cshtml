@model CRUD_MVC.Category

@{
    ViewBag.Title = "Category Details";
}

<h2>Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ID :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt>
           Category Name :
        </dt>

